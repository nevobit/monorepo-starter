{
  "name": "@{{packageName}}/design-system",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/shared/index.cjs",
  "module": "dist/shared/index.js",
  "types": "dist/shared/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/shared/index.d.ts",
      "import": "./dist/shared/index.js",
      "require": "./dist/shared/index.cjs"
    },
    "./web": {
      "types": "./dist/web/index.d.ts",
      "import": "./dist/web/index.js",
      "require": "./dist/web/index.cjs"
    },
    "./native": {
      "types": "./dist/native/index.d.ts",
      "import": "./dist/native/index.js",
      "require": "./dist/native/index.cjs"
    },
    "./css/web.css": "./styles/web.css",
    "./css/tokens.css": "./styles/tokens.css"
  },
  "files": [
    "dist",
    "styles"
  ],
  "scripts": {
    "build:tokens": "tsx src/shared/tokens/build.ts",
    "prebuild": "pnpm run build:tokens",
    "build": "tsup src/web/index.ts src/native/index.ts src/shared/index.ts --dts --format esm,cjs --sourcemap --tsconfig tsconfig.build.json",
    "dev": "pnpm run build:tokens && tsup src/web/index.ts src/native/index.ts src/shared/index.ts --dts --format esm,cjs --sourcemap --watch --tsconfig tsconfig.dev.json",
    "type-check": "tsc -p tsconfig.build.json --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.5.0",
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@{{packageName}}/typescript-config": "workspace:*",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-vitest": "^9.1.3",
    "@storybook/react-vite": "^9.1.3",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "clsx": "^2.1.1",
    "eslint-plugin-storybook": "^9.1.3",
    "playwright": "^1.55.0",
    "storybook": "^9.1.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "react": "catalog:"
  }
}
